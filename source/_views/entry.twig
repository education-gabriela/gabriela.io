    <![endif]-->
    <link rel="stylesheet" href="{{ site.url }}/components/highlightjs/styles/monokai.css" />
    <link rel="alternate" type="application/atom+xml" href="{{ site.url }}/atom.xml"
          title="{{ site.title }} activity feed"/>
    <style>
        /** quick fix because bootstrap <pre> has a background-color. */
        pre code {
            background-color: inherit;
            font-size: 80%;
        }
    </style>
    <script type="text/javascript">
        //<![CDATA[
        (function() {
            var shr = document.createElement('script');
            shr.setAttribute('data-cfasync', 'false');
            shr.src = '//dsms0mj1bbhn4.cloudfront.net/assets/pub/shareaholic.js';
            shr.type = 'text/javascript'; shr.async = 'true';
            shr.onload = shr.onreadystatechange = function() {
                var rs = this.readyState;
                if (rs && rs != 'complete' && rs != 'loaded') return;
                var site_id = '28c8aab7860c17689aeeff914dc00b2c';
                try { Shareaholic.init(site_id); } catch (e) {}
            };
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(shr, s);
        })();
        //]]>
    </script>

</head>
<body id="page-top home" class="index blog-page">


{% include "personal/navbar.twig" %}

<section id="blog" class="light-bg">

    <div class="container">

        <div class="row blog">

            <div class="col-sm-8 blog-main">

                {% block content_wrapper %}{% block content %}{% endblock %}{% endblock %}

            </div><!-- /.blog-main -->

            <div class="col-sm-4 blog-sidebar">
                {% include 'personal/sidebar.twig' %}
            </div><!-- /.blog-sidebar -->

        </div><!-- /.row -->

    </div><!-- /.container -->
</section>

    {% include "footer.twig" %}

    {% include "personal/footer_scripts.twig" %}

    {% include "google.twig" %}

</body>

</html>